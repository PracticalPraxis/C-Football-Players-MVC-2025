@model IEnumerable<ArsenalPlayers.Models.APIModels.IntermediaryAPIObject>

@{
    ViewData["Title"] = "ArsenalAPI";
}

<h1>ArsenalAPI</h1>

<table class="table table-hover" style="width:100%" id="APITable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.utcDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.matchday)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.group)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lastUpdated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.areaname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.competitionname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.homeTeamname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.awayTeamname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.scorewinner)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.scoreduration)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.utcDate)
                </td>
                <td class="JerseyNumberTD">
                    @Html.DisplayFor(modelItem => item.status)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.matchday)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.group)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.lastUpdated)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.areaname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.competitionname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.homeTeamname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.awayTeamname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.scorewinner)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.scoreduration)
                </td>
            </tr>
        }
    </tbody>
</table>
